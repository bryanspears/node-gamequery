{
  "env": {
    "browser": true
  }, 
  "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "rules": {
    "class-methods-use-this": 1,
    "dot-notation": 1,
    "eqeqeq": 2,
    "curly": 2,
    "prefer-const": 1,
    "prefer-arrow-callback": 1,
    "no-var": 2,
    "no-undef": 0,
    "quotes": 0,
    "strict": 0,
    "no-unused-vars": [2, {"vars": "local", "args": "none"}],
    "camelcase": 0,
    "no-console": 0
  }
}
